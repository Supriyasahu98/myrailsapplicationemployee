<u><h1>Listing of Dependents Data</h1></u>
<body bgcolor="pink" >
<%= link_to 'Add New Dependent Here', new_dependent_path %>
<br>
<table border="2">
<tr><br>
 
    <th>Essn</th>
    <th>Dependent_name</th>
    <th>Sex</th>
    <th>Bdate</th>
    <th colspan="3">Options</th>
</tr>
 
<% @dependents.each do |d| %>
<tr bgcolor="white">
      <td><%= d.Essn %></td>
      <td><%= d.Dependent_name %></td>
      <td><%= d.Sex %></td>
      <td><%= d.Bdate %></td>
      
      <td><%= link_to 'show', dependent_path(d) %></td>
      <td><%= link_to 'edit', edit_dependent_path(d) %></td>
      <td><%= link_to 'Destroy', dependent_path(d), method: 
      :delete, data: { confirm: 'Are you Sure to delete this dependent Details' } %> </td>
      </tr>
      <% end %>
      </table>
      </body>
 
 
 