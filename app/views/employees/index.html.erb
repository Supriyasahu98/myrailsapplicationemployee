<u><h1>Listing of Employees Data</h1></u>
<body bgcolor="pink" >
<%= link_to 'Add New Employee Here', new_employee_path %>
<br>
<table border="2">
<tr><br>
 
    <th>Fname</th>
    <th>Lname</th>
    <th>ESSN</th>
    <th>Birth_Date</th>
    <th>Address</th>
    <th>Sex</th>
    <th>Salary</th>
    <th>DeptNo</th>
      <th colspan="3">Options</th>
 
</tr>
 
<% @employees.each do |e| %>
<tr bgcolor="white">
      <td><%= e.Fname %></td>
      <td><%= e.Lname %></td>
      <td><%= e.Essn %></td>
      <td ><%= e.Bdate %></td>
      <td><%= e.Address %></td>
      <td><%= e.Sex %></td>
      <td><%= e.Salary %></td>
      <td><%= e.Dnumber %></td>
      <td><%= link_to 'show', employee_path(e) %></td>
      <td><%= link_to 'edit', edit_employee_path(e) %></td>
      <td><%= link_to 'Destroy', employee_path(e), method: :delete, data: {confirm: 'Are you Sure to delete this Employee Details' } %></td>
      </tr>
      <% end %>
      </table>
      </body>
 
 
 